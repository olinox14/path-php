{
  "name": "olinox14/path-php",
  "description": "Object-oriented file and path operations, inspired by the path.py python library",
  "require": { "php": ">=8.0" },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Olivier Massot",
      "email": "olinox14@tuta.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "Path\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Path\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "ext-posix": "*",
    "php-coveralls/php-coveralls": "^2.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
